#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#define d (b*b-4*a*c)
int main()
{
	int a,b,c;
	float root1,root2;
	printf("enter the a,b,c in ax^2+bx+c: ");
	scanf("%d,%d,%d",&a,&b,&c);
	if(d > 0)
	{
		root1= (-b+sqrt(d))/2*a;
		root2= (-b-sqrt(d))/2*a;
		printf("the roots are: %d,%.2f,%.2f\n",d,root1,root2);
	}
	if(d<0)
	{
		printf("the roots for these coordinate are not possible");
		printf("\n");
	}
}